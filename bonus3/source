#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int		main(int argc, char **argv)
{
	FILE *file = fopen("/home/user/end/.pass", "r");
	if (!argv[1] || !file)
		return (-1);
	char data[67];
	fread(data, 1, 66, file);
	data[66] = 0;
	*(char*)(atoi(argv[1]) + 24) = 0;
	char data2[66];
	fread(data2, 1, 65, file);
	if (strcmp(data, argv[1]) == 0)
	{
		execl("/bin/sh", "sh");
	}
	else
	{
		puts(data2);
	}
	return (1);
}
